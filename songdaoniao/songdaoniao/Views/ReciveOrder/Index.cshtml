@using ClassLibrary1

@{
    Layout = null;
    ViewBag.Title = "Index";
    List<order> order = ViewBag.List;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table>
            <thead>
                <tr>
                    <td>订单号</td>
                    <td>日期</td>
                    <td>类型</td>
                    <td>内容</td>
                    <td>收件人ID</td>
                    <td>收件人电话</td>
                    <td>打赏</td>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (order li in order)
                {
                <tr>
                    <td>@li.OrderNumber</td>
                    <td>@li.Date</td>
                    <td>@li.Type</td>
                    <td>@li.Text</td>
                    <td>@li.ClientID</td>
                    <td>@li.ClientPhone</td>
                    <td>@li.Tip</td>
                    <td><input id="button" type="submit" value="接订单" /></td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
